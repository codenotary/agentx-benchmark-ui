{
  "name": "agentx-benchmark-ui",
  "private": false,
  "version": "1.0.0",
  "description": "Benchmark Dashboard for AgentX - Multi-Provider LLM Performance Comparison",
  "homepage": "https://codenotary.github.io/agentx-benchmark-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codenotary/agentx-benchmark-ui.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:local": "vite",
    "dev:static": "VITE_USE_STATIC_SQLITE=true vite --host",
    "server": "node server.cjs",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "update-db": "bash scripts/update-db.sh",
    "build": "tsc -b && vite build",
    "build:static": "npm run update-db && VITE_USE_STATIC_SQLITE=true vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build:static",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-query-devtools": "^5.89.0",
    "@tanstack/react-router": "^1.131.47",
    "clsx": "^2.1.1",
    "comlink": "^4.4.2",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.2.1",
    "sql.js": "^1.13.0",
    "sql.js-httpvfs": "^0.8.12",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6"
  }
}
